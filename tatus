[32m+[m
[32m+[m[32m### 10. POST /api/events/:id/register[m
[32m+[m[32m**Purpose:** Register alumni as judge for an event (sends email to admin)[m
[32m+[m
[32m+[m[32m**Request:**[m
[32m+[m[32m```json[m
[32m+[m[32m{[m
[32m+[m[32m  "alumniEmail": "alumni@example.com",[m
[32m+[m[32m  "alumniName": "John Doe",[m
[32m+[m[32m  "preferredDateTime": "2024-03-15T09:00:00Z",[m
[32m+[m[32m  "preferredLocation": "Main Hall"[m
[32m+[m[32m}[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m**Response:**[m
[32m+[m[32m```json[m
[32m+[m[32m{[m
[32m+[m[32m  "success": true,[m
[32m+[m[32m  "message": "Registration successful. Admin has been notified.",[m
[32m+[m[32m  "data": {[m
[32m+[m[32m    "eventId": "EVT-1234567890-abc123",[m
[32m+[m[32m    "eventTitle": "Case Competition 2024"[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m**PowerShell:**[m
[32m+[m[32m```powershell[m
[32m+[m[32m$registerBody = @{[m
[32m+[m[32m    alumniEmail = "alumni@example.com"[m
[32m+[m[32m    alumniName = "John Doe"[m
[32m+[m[32m    preferredDateTime = "2024-03-15T09:00:00Z"[m
[32m+[m[32m    preferredLocation = "Main Hall"[m
[32m+[m[32m} | ConvertTo-Json[m
[32m+[m
[32m+[m[32mInvoke-RestMethod -Uri "http://localhost:3000/api/events/EVT-1234567890-abc123/register" -Method POST -Body $registerBody -ContentType "application/json" | ConvertTo-Json[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m---[m
[32m+[m
[32m+[m[32m## üìä DATA STRUCTURES[m
[32m+[m
[32m+[m[32m### Event Object Structure[m
[32m+[m[32m```json[m
[32m+[m[32m{[m
[32m+[m[32m  "eventId": "string (auto-generated: EVT-timestamp-random)",[m
[32m+[m[32m  "eventInfo": {[m
[32m+[m[32m    "name": "string (required)",[m
[32m+[m[32m    "description": "string (required)",[m
[32m+[m[32m    "date": "string (ISO 8601, optional)"[m
[32m+[m[32m  },[m
[32m+[m[32m  "teams": [[m
[32m+[m[32m    {[m
[32m+[m[32m      "teamId": "string (required)",[m
[32m+[m[32m      "teamName": "string (required)",[m
[32m+[m[32m      "members": ["string array of student IDs"][m
[32m+[m[32m    }[m
[32m+[m[32m  ],[m
[32m+[m[32m  "rubrics": [[m
[32m+[m[32m    {[m
[32m+[m[32m      "rubricId": "string (required)",[m
[32m+[m[32m      "name": "string (required)",[m
[32m+[m[32m      "maxScore": "number (required, > 0)",[m
[32m+[m[32m      "weight": "number (required, >= 0)"[m
[32m+[m[32m    }[m
[32m+[m[32m  ],[m
[32m+[m[32m  "judges": [[m
[32m+[m[32m    {[m
[32m+[m[32m      "judgeId": "string (required)",[m
[32m+[m[32m      "status": "string (required: 'approved' | 'pending')"[m
[32m+[m[32m    }[m
[32m+[m[32m  ],[m
[32m+[m[32m  "scores": [[m
[32m+[m[32m    {[m
[32m+[m[32m      "judgeId": "string (required)",[m
[32m+[m[32m      "teamId": "string (required)",[m
[32m+[m[32m      "rubricId": "string (required)",[m
[32m+[m[32m      "score": "number (required, >= 0, <= maxScore)",[m
[32m+[m[32m      "timestamp": "string (ISO 8601, auto-generated)"[m
[32m+[m[32m    }[m
[32m+[m[32m  ],[m
[32m+[m[32m  "createdAt": "string (ISO 8601, auto-generated)",[m
[32m+[m[32m  "updatedAt": "string (ISO 8601, auto-generated)"[m
[32m+[m[32m}[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m---[m
[32m+[m
[32m+[m[32m## üîÑ COMPLETE WORKFLOW EXAMPLES[m
[32m+[m
[32m+[m[32m### Example 1: Create Event with Teams and Rubrics[m
[32m+[m[32m```powershell[m
[32m+[m[32m$eventBody = @{[m
[32m+[m[32m    eventInfo = @{[m
[32m+[m[32m        name = "Spring Case Competition 2024"[m
[32m+[m[32m        description = "Annual spring case competition"[m
[32m+[m[32m        date = "2024-04-15T09:00:00Z"[m
[32m+[m[32m    }[m
[32m+[m[32m    teams = @([m
[32m+[m[32m        @{[m
[32m+[m[32m            teamId = "TEAM-001"[m
[32m+[m[32m            teamName = "Team Alpha"[m
[32m+[m[32m            members = @("STU-001", "STU-002", "STU-003")[m
[32m+[m[32m        },[m
[32m+[m[32m        @