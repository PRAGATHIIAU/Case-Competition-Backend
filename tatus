[32m+[m[32m      "teamName": "Team Alpha",[m
[32m+[m[32m      "members": ["STU-001", "STU-002"],[m
[32m+[m[32m      "finalScore": 87.5[m
[32m+[m[32m    },[m
[32m+[m[32m    {[m
[32m+[m[32m      "rank": 2,[m
[32m+[m[32m      "teamId": "TEAM-002",[m
